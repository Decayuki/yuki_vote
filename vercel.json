{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "server/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "server/server.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "server/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server/public/$1"
        }
    ]
}
